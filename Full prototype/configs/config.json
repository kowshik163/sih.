{
  "model": {
    "name": "FRA_Unified_AI_Enhanced",
    "version": "2.0.0",
    "hidden_size": 1024,
    "num_ner_labels": 15,
    "num_schemes": 50,
    "max_sequence_length": 512,
    "temperature": 0.7,
    "device": "auto",
    "unified_token_fusion": true,
    "multimodal_pretraining": true,
    "geospatial_aware": true,
    "temporal_modeling": {
      "enabled": true,
      "window_size": 12,
      "d_model": 256
    },
    "graph_neural_network": {
      "enabled": true,
      "num_layers": 3,
      "hidden_dim": 512,
      "k_neighbors": 8
    },
    "memory_module": {
      "enabled": true,
      "capacity": 4096,
      "embedding_dim": 1024,
      "retrieval_k": 16
    },
    "contrastive_learning": {
      "temperature": 0.07,
      "margin": 0.2,
      "negative_samples": 8
    },
    "knowledge_graph": {
      "enabled": true,
      "embedding_dim": 256,
      "num_relations": 20
    }
  },
  "llm_config": {
    "primary_model": {
      "name": "mistralai/Mistral-7B-Instruct-v0.1",
      "max_tokens": 4096,
      "temperature": 0.7,
      "top_p": 0.9,
      "top_k": 40,
      "repetition_penalty": 1.1,
      "use_cache": true,
      "device_map": "auto",
      "load_in_8bit": true,
      "torch_dtype": "float16"
    },
    "rag_config": {
      "vector_db": "faiss",
      "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
      "chunk_size": 512,
      "chunk_overlap": 50,
      "retrieval_k": 5,
      "score_threshold": 0.7
    },
    "fine_tuning": {
      "method": "lora",
      "r": 16,
      "lora_alpha": 32,
      "lora_dropout": 0.1,
      "target_modules": ["q_proj", "v_proj", "k_proj", "o_proj", "gate_proj", "up_proj", "down_proj"],
      "learning_rate": 2e-4,
      "num_epochs": 3,
      "per_device_train_batch_size": 1,
      "gradient_accumulation_steps": 8
    },
    "tasks": {
      "document_qa": {
        "enabled": true,
        "system_prompt": "You are an expert FRA (Forest Rights Act) assistant. Answer questions based on the provided documents and context."
      },
      "spatial_query": {
        "enabled": true,
        "system_prompt": "You are a GIS expert specializing in FRA claims and forest rights. Convert natural language queries to PostGIS SQL."
      },
      "policy_analysis": {
        "enabled": true,
        "system_prompt": "You are a policy analyst expert in FRA implementation and CSS schemes. Provide recommendations based on data."
      },
      "ner_extraction": {
        "enabled": true,
        "system_prompt": "Extract named entities from FRA documents: village names, patta holders, coordinates, claim status, dates."
      }
    }
  },
  "ocr_config": {
    "primary_engine": "layoutlmv3",
    "fallback_engine": "tesseract",
    "preprocessing": {
      "denoise": true,
      "contrast_enhancement": true,
      "skew_correction": true,
      "deskew_angle_threshold": 5
    },
    "postprocessing": {
      "spell_check": true,
      "entity_linking": true,
      "confidence_threshold": 0.6
    },
    "languages": ["eng", "hin", "ben", "tel", "mar", "guj", "tam", "kan", "mal", "ori"],
    "models": {
      "document_layout": "microsoft/layoutlmv3-base",
      "text_recognition": "microsoft/trocr-base-stage1",
      "table_detection": "microsoft/table-transformer-structure-recognition"
    }
  },
  "computer_vision": {
    "satellite_analysis": {
      "primary_model": "facebook/deeplabv3-resnet50-ade",
      "land_cover_model": "qualcomm/DeepLabV3-ResNet50",
      "change_detection_model": "nvidia/segformer-b0-finetuned-ade-512-512",
      "asset_detection_model": "facebook/detr-resnet-50",
      "foundation_model": "ibm-nasa-geospatial/Prithvi-100M"
    },
    "preprocessing": {
      "cloud_masking": true,
      "atmospheric_correction": true,
      "radiometric_normalization": true,
      "geometric_correction": true
    },
    "land_use_classes": {
      "forest": 1,
      "agriculture": 2,
      "water": 3,
      "built_up": 4,
      "barren": 5,
      "grassland": 6,
      "wetland": 7,
      "snow_ice": 8,
      "other": 0
    },
    "indices": ["NDVI", "NDWI", "EVI", "SAVI", "NDBI", "BSI", "MNDWI", "GNDVI"]
  },
  "training": {
    "learning_rate": 0.0001,
    "batch_size": 4,
    "num_epochs": {
      "stage_0_pretraining": 15,
      "stage_1_foundation": 10,
      "stage_2_alignment": 8,
      "stage_3_tool_skills": 5,
      "stage_4_dss": 5
    },
    "multimodal_pretraining": {
      "contrastive_weight": 1.0,
      "masked_token_weight": 0.5,
      "cross_modal_weight": 0.3
    },
    "weight_decay": 0.01,
    "gradient_clip": 1.0,
    "warmup_steps": 1000,
    "save_every_n_epochs": 2,
    "validate_every_n_epochs": 1,
    "checkpoint_dir": "./checkpoints",
    "log_dir": "./logs"
  },
  "data": {
    "raw_data_dir": "./data/raw",
    "processed_data_dir": "./data/processed",
    "training_data_path": "./data/training_data.json",
    "validation_split": 0.2,
    "test_split": 0.1,
    "max_image_size": [
      224,
      224
    ],
    "supported_formats": {
      "documents": [
        ".pdf",
        ".png",
        ".jpg",
        ".jpeg",
        ".tiff"
      ],
      "satellite": [
        ".tif",
        ".tiff",
        ".jp2"
      ],
      "vector": [
        ".shp",
        ".geojson",
        ".gpkg"
      ]
    }
  },
  "database": {
    "type": "postgresql",
    "host": "localhost",
    "port": 5432,
    "name": "fra_gis",
    "user": "fra_user",
    "password": "fra_password",
    "pool_size": 10,
    "max_overflow": 20
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "cors_origins": [
      "*"
    ],
    "max_upload_size": 52428800,
    "rate_limit": {
      "requests_per_minute": 100,
      "burst_size": 20
    }
  },
  "external_services": {
    "geoserver": {
      "url": "http://localhost:8080/geoserver",
      "username": "admin",
      "password": "geoserver",
      "workspace": "fra_workspace"
    },
    "tile_servers": {
      "osm": "https://tile.openstreetmap.org/{z}/{x}/{y}.png",
      "satellite": "https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
    }
  },
  "processing": {
    "ocr": {
      "engine": "tesseract",
      "languages": [
        "eng",
        "hin"
      ],
      "confidence_threshold": 30,
      "preprocessing": {
        "denoise": true,
        "contrast_enhancement": true,
        "skew_correction": true
      }
    },
    "satellite": {
      "bands": {
        "sentinel2": [
          "B2",
          "B3",
          "B4",
          "B8",
          "B11",
          "B12"
        ],
        "landsat8": [
          "B2",
          "B3",
          "B4",
          "B5",
          "B6",
          "B7"
        ]
      },
      "indices": [
        "NDVI",
        "NDWI",
        "EVI",
        "SAVI"
      ],
      "cloud_threshold": 20
    },
    "parallel": {
      "num_workers": 4,
      "batch_size": 16
    }
  },
  "ner_labels": {
    "village_name": 0,
    "patta_holder": 1,
    "claim_type": 2,
    "status": 3,
    "coordinates": 4,
    "area": 5,
    "date": 6,
    "district": 7,
    "state": 8,
    "survey_number": 9,
    "revenue_village": 10,
    "block": 11,
    "tehsil": 12,
    "forest_range": 13,
    "other": 14
  },
  "css_schemes": {
    "PM-KISAN": {
      "id": 0,
      "ministry": "Agriculture & Farmers Welfare",
      "beneficiary_type": "Individual",
      "amount": 6000,
      "frequency": "Annual"
    },
    "Jal Jeevan Mission": {
      "id": 1,
      "ministry": "Jal Shakti",
      "beneficiary_type": "Household",
      "amount": "Variable",
      "frequency": "One-time"
    },
    "MGNREGA": {
      "id": 2,
      "ministry": "Rural Development",
      "beneficiary_type": "Individual",
      "amount": "Variable",
      "frequency": "Daily wages"
    },
    "DAJGUA": {
      "id": 3,
      "ministry": "Multiple",
      "beneficiary_type": "Community",
      "amount": "Variable",
      "frequency": "Project-based"
    }
  },
  "land_use_classes": {
    "forest": 1,
    "agriculture": 2,
    "water": 3,
    "built_up": 4,
    "barren": 5,
    "other": 0
  },
  "model_sources": {
    "primary_llm": "mistralai/Mistral-7B-Instruct-v0.1",
    "secondary_llm": "meta-llama/Llama-2-7b-chat-hf",
    "backup_llm": "tiiuae/falcon-7b-instruct",
    "layoutlm": "microsoft/layoutlmv3-base",
    "layoutlm_large": "microsoft/layoutlmv3-large",
    "trocr": "microsoft/trocr-base-stage1",
    "trocr_large": "microsoft/trocr-large-stage1",
    "ocr_ner": "ai4bharat/indic-bert",
    "legal_ner": "opennyaiorg/en_legal_ner_trf",
    "legal_translation": "law-ai/InLegalTrans-En2Indic-1B",
    "bert_base": "bert-base-uncased",
    "bert_multilingual": "bert-base-multilingual-cased",
    "indic_bert": "ai4bharat/indic-bert",
    "roberta_base": "roberta-base",
    "distilbert": "distilbert-base-uncased",
    "clip": "openai/clip-vit-base-patch32",
    "clip_large": "openai/clip-vit-large-patch14",
    "detr": "facebook/detr-resnet-50",
    "detr_panoptic": "facebook/detr-resnet-50-panoptic",
    "deeplabv3": "facebook/deeplabv3-resnet50-ade",
    "deeplabv3_satellite": "qualcomm/DeepLabV3-ResNet50",
    "unet_satellite": "nvidia/segformer-b0-finetuned-ade-512-512",
    "swin_transformer": "microsoft/swin-base-patch4-window7-224",
    "swin_large": "microsoft/swin-large-patch4-window12-384",
    "resnet50": "microsoft/resnet-50",
    "resnet101": "microsoft/resnet-101",
    "vit_base": "google/vit-base-patch16-224",
    "vit_large": "google/vit-large-patch16-224",
    "sam_model": "facebook/sam-vit-huge",
    "segment_anything": "facebook/sam-vit-base",
    "geospatial_fm": "ibm-nasa-geospatial/Prithvi-100M",
    "satellite_foundation": "microsoft/torchgeo-resnet50-sentinel2-all",
    "spacy_en": "en_core_web_sm",
    "spacy_hindi": "hi_core_news_sm",
    "spacy_multilingual": "xx_ent_wiki_sm"
  },
  "data_sources": {
    "fra_datasets": {
      "type": "collection",
      "description": "Real FRA documents and legal datasets",
      "sources": [
        {
          "name": "AI4Bharat_IndicNLP",
          "type": "git",
          "url": "https://github.com/AI4Bharat/indicnlp_corpus",
          "description": "Large-scale Indic language corpora suitable for OCR text"
        },
        {
          "name": "FRA_Legal_Corpus",
          "type": "huggingface",
          "url": "https://huggingface.co/datasets/opennyaiorg/InLegalNER",
          "description": "Annotated NER datasets for Indian legal texts"
        },
        {
          "name": "InLegal_Translation",
          "type": "huggingface", 
          "url": "https://huggingface.co/law-ai/InLegalTrans-En2Indic-1B",
          "description": "Legal text translation model for 9 Indian languages"
        }
      ]
    },
    "ocr_datasets": {
      "type": "collection",
      "description": "OCR training datasets for document processing",
      "sources": [
        {
          "name": "ICDAR_2019_RRC-MLT",
          "type": "http",
          "url": "https://rrc.cvc.uab.es/?ch=15&com=downloads",
          "description": "Multilingual text detection and recognition"
        },
        {
          "name": "IIIT_Hindi_OCR",
          "type": "http", 
          "url": "https://cvit.iiit.ac.in/research/projects/cvit-projects/indic-ocr-datasets",
          "description": "Hindi OCR datasets from IIIT Hyderabad"
        },
        {
          "name": "Bengali_OCR",
          "type": "http",
          "url": "https://www.isical.ac.in/~cvpr/project/bbox.html", 
          "description": "Bengali OCR with bounding box annotations"
        }
      ]
    },
    "satellite_imagery": {
      "type": "collection",
      "description": "Satellite imagery and geospatial datasets for land use analysis",
      "sources": [
        {
          "name": "IndiaSAT_Dataset",
          "type": "git",
          "url": "https://github.com/ICTD-IITD/IndiaSAT",
          "description": "Indian satellite imagery dataset for classification"
        },
        {
          "name": "BHUVAN_Free_Data",
          "type": "http",
          "url": "https://bhuvan.nrsc.gov.in/wiki/index.php/Free_Satellite_Data_Download",
          "description": "Free satellite data from NRSC India"
        },
        {
          "name": "NRSC_LULC_Atlas", 
          "type": "http",
          "url": "https://www.nrsc.gov.in/Atlas_LULC",
          "description": "Land Use Land Cover Atlas from NRSC"
        },
        {
          "name": "VEDAS_Portal",
          "type": "http", 
          "url": "https://vedas.sac.gov.in/en/download.html",
          "description": "ISRO Earth observation data portal"
        }
      ]
    },
    "graph_databases": {
      "type": "collection",
      "description": "Graph database examples and knowledge graph datasets",
      "sources": [
        {
          "name": "Neo4j_Examples",
          "type": "git",
          "url": "https://github.com/neo4j/graph-examples",
          "description": "Neo4j graph database examples and use cases"
        },
        {
          "name": "NetworkX_Examples", 
          "type": "git",
          "url": "https://github.com/networkx/networkx/tree/main/examples",
          "description": "Python NetworkX graph analysis examples"
        }
      ]
    },
    "business_rules": {
      "type": "collection", 
      "description": "Business rules datasets and ML training data",
      "sources": [
        {
          "name": "UCI_Adult_Dataset",
          "type": "http",
          "url": "https://archive.ics.uci.edu/ml/datasets/adult",
          "description": "Adult census income dataset for classification"
        },
        {
          "name": "Credit_Approval_Dataset",
          "type": "http",
          "url": "https://archive.ics.uci.edu/ml/datasets/credit+approval", 
          "description": "Credit card approval dataset"
        },
        {
          "name": "Bank_Marketing_Dataset",
          "type": "http",
          "url": "https://archive.ics.uci.edu/ml/datasets/bank+marketing",
          "description": "Bank marketing campaign dataset"
        }
      ]
    },
    "geospatial_boundaries": {
      "type": "collection",
      "description": "Administrative and geographic boundary datasets",
      "sources": [
        {
          "name": "OpenStreetMap_India",
          "type": "http", 
          "url": "https://download.geofabrik.de/asia/india.html",
          "description": "OpenStreetMap data for India including administrative boundaries"
        },
        {
          "name": "DataMeet_Boundaries",
          "type": "git",
          "url": "https://github.com/datameet/maps",
          "description": "Indian administrative boundary shapefiles"
        },
        {
          "name": "Survey_of_India_Maps",
          "type": "http",
          "url": "https://www.surveyofindia.gov.in/pages/display/57-digital-mapping-data",
          "description": "Official Survey of India digital mapping data"
        }
      ]
    },
    "nlp_language_models": {
      "type": "collection",
      "description": "NLP datasets and language models for Indian languages",
      "sources": [
        {
          "name": "IndicNLP_Corpus",
          "type": "git", 
          "url": "https://github.com/AI4Bharat/indicnlp_corpus",
          "description": "Large-scale multilingual Indian language corpora"
        },
        {
          "name": "MILPaC_Legal_Corpus",
          "type": "git",
          "url": "https://github.com/Law-AI/MILPaC", 
          "description": "Multilingual Indian Legal Parallel Corpus"
        },
        {
          "name": "Anuvaad_Parallel_Corpus",
          "type": "git",
          "url": "https://github.com/project-anuvaad/anuvaad-parallel-corpus",
          "description": "Parallel corpora for Indic languages including legal texts"
        },
        {
          "name": "OPUS_Indic_Corpus",
          "type": "http",
          "url": "https://opus.nlpl.eu/",
          "description": "OPUS Open Parallel Corpus for Indic-English language pairs"
        }
      ]
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "./logs/fra_fusion.log",
    "max_size": "10MB",
    "backup_count": 5
  },
  "security": {
    "jwt_secret": "your-secret-key-here",
    "jwt_expire_hours": 24,
    "allowed_ips": [],
    "encrypt_sensitive_data": true
  },
  "performance": {
    "cache": {
      "enabled": true,
      "type": "redis",
      "ttl": 3600,
      "max_size": 1000
    },
    "model_optimization": {
      "quantization": true,
      "pruning": false,
      "onnx_export": false
    }
  }
}
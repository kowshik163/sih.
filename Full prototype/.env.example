# FRA AI System Environment Variables
# Copy this to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://fra_user:fra_password@localhost:5432/fra_gis
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fra_gis
DB_USER=fra_user
DB_PASSWORD=fra_password

# Security
SECRET_KEY=your-super-secret-key-change-this-in-production
JWT_SECRET_KEY=your-jwt-secret-key-change-this
JWT_EXPIRE_HOURS=24

# Hugging Face Hub
HF_TOKEN=your-hugging-face-token-here

# External Services
GEOSERVER_URL=http://localhost:8080/geoserver
GEOSERVER_USERNAME=admin
GEOSERVER_PASSWORD=geoserver

# API Configuration  
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true
CORS_ORIGINS=*

# Model Paths (will be auto-populated by download script)
MODEL_CACHE_DIR=./models
CHECKPOINT_DIR=./checkpoints

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/fra_fusion.log

# Development
ENVIRONMENT=development
